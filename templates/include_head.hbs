    <link rel="stylesheet" href="{{site.info.base_url}}/static/css/poole.css">
    <link rel="stylesheet" href="{{site.info.base_url}}/static/css/hyde.css">
    <link rel=" stylesheet" href="https://fonts.googleapis.com/css?family=PT+Sans:400,400italic,700|Abril+Fatface">
      <!-- Icons -->
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{site.info.base_url}}/static/apple-touch-icon-144-precomposed.png">
                                 <link rel="shortcut icon" href="{{site.info.base_url}}/static/favicon.ico">

